{
    "ServerURL": "https://0.0.0.0:5000/",
    "HeatbeatInterval": 10,
    "Mqtt": {
        "host": "mosquitto",
        "port": "1883",
        "clientID": "MQTTValidationService",
        "messagesTopic": "status/mqttvalidationservice/messages",
        "heartBeatTopic": "status/mqttvalidationservice/heartbeats",
        "controlTopic": "control/mqttvalidationservice"
    },
    "Logging": {
        "Console": {
            "IncludeScopes": false,
            "LogLevel": {
                "Default": "Information"
            }
        },
        "File": {
            "LogLevel": {
                "Default": "Information",
                "System": "Warning",
                "Microsoft": "Warning"
            },
            "Shared": false,
            "Template": "{Timestamp:o} {RequestId,13} [{Level:u3}] {Message} ({EventId:x8}){NewLine}{Exception}",
            "FlushInterval": 2,
            "PathFormat": "Logs/MQTTValidationService.log",
            "FileCountLimit": 31,
            "FileSizeLimit": 1073741824,
            "IncludeScopes": false,
            "BufferSize": null
        }
    }
}