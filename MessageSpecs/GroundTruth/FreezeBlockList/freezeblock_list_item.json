{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://asist.aptima.com/schemas/mission_victim.json",
    "type": "object",
    "title": "Freeze Block List Item Schema",
    "version": "0.1",
    "required": [
      "x",
      "y",
      "z",
      "block_type"
    ],
    "properties": {
        "x": {
            "$id": "#/properties/x",
            "type": "number",
            "title": "The X Schema",
            "default": 0.0,
            "examples": [
              -2194.8933503172193
            ]
          },
          "y": {
            "$id": "#/properties/y",
            "type": "number",
            "title": "The Y Schema",
            "default": 0.0,
            "examples": [
              24.0
            ]
          },
          "z": {
            "$id": "#/properties/z",
            "type": "number",
            "title": "The Z Schema",
            "default": 0.0,
            "examples": [
              190.33727
            ]
          },
          "block_type": {
            "$id": "#/properties/block_type",
            "type": "string",
            "title": "The Block Type Schema",          
            "examples": [
              "block_freeze_player"
            ],
            "pattern": "^(.*)$"
          },
          "room_name": {
            "$id": "#/properties/room_name",
            "type": "string",
            "title": "The Room Name Schema",          
            "examples": [
              "Security Office"
            ],
            "pattern": "^(.*)$"
          },
          "feature_type":{
            "$id": "#/properties/feature_type",
            "type": "string",
            "title": "The Feature Type Schema",          
            "examples": [
              "Threat Room"
            ],
            "pattern": "^(.*)$"
          }

    }
  }
  