{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://asist.aptima.com/schemas/M3.json",
    "type": "object",
    "title": "The M3 Schema",
    "version": "0.5",
    "required": [
        "player_name", "map_name"
    ],
    "properties": {
        "player_name": {
            "$id": "#/properties/player_name",
            "type": "string",
            "title": "The player name",          
            "examples": ["Player123"],
            "pattern": "^(.*)$"
        },
        "map_name": {
            "$id": "#/properties/map_name",
            "type": "string",
            "title": "The map name",          
            "examples": ["SaturnA_24"],
            "pattern": "^(.*)$"
        }
    }
}