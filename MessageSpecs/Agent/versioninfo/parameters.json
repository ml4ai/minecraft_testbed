{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://asist.aptima.com/schemas/parameters.json",
  "type": "object",
  "title": "The agent parameters Schema",
  "version": "1.0",
  "required": [
    "name",
	"value"
  ],
  "properties": {
    "name": {
      "$id": "#/properties/agent/version/parameter/name",
      "type": "string",
      "title": "The agent version parameter name",
      "pattern": "^(.*)$"
    },
    "value": {
      "$id": "#/properties/agent/version/parameter/value",
      "type": "string",
      "title": "The parameter value",
      "pattern": "^(.*)$"
    }
  }
}
